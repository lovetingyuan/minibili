{
  "extends": "expo/tsconfig.base",
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts",
    "*.d.ts",
    ".expo/types/**/*.ts",
  ],
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  }
}
