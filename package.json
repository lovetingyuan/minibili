{
  "name": "minibili",
  "version": "0.0.13",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "adb devices -l && npx expo start -c --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild:android": "zx scripts/update-version.mjs --pre",
    "postbuild:android": "zx scripts/update-version --post",
    "build:android": "npm run test && eas build --platform android --profile production --json --non-interactive",
    "fixdeps": "npx expo install --fix",
    "doctor": "expo-cli doctor",
    "docs": "npx vite docs --base /minibili/",
    "build:site": "cd site && npm run build",
    "format": "npx prettier src --write",
    "test": "npx vitest run",
    "lint": "eslint --ext ts,tsx,js,jsx --fix src"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "9.3.7",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "@sentry/react-native": "4.13.0",
    "@shopify/flash-list": "1.4.0",
    "expo": "~48.0.9",
    "expo-application": "~5.1.1",
    "expo-background-fetch": "~11.1.1",
    "expo-clipboard": "~4.1.2",
    "expo-constants": "~14.2.1",
    "expo-device": "~5.2.1",
    "expo-file-system": "~15.2.2",
    "expo-keep-awake": "~12.0.1",
    "expo-notifications": "~0.18.1",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "p-queue": "6.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.6",
    "react-native-exception-handler": "^2.10.10",
    "react-native-pager-view": "6.1.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-webview": "11.26.0",
    "sentry-expo": "~6.1.0",
    "swr": "^2.1.1",
    "url-regex": "^5.0.0",
    "valtio": "^1.7.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/react": "^18.0.31",
    "@types/react-native": "~0.71.4",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "happy-dom": "^9.1.9",
    "prettier": "^2.8.7",
    "typescript": "^4.9.4",
    "vitest": "^0.29.7",
    "zx": "^7.2.1"
  }
}
