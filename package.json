{
  "name": "minibili",
  "version": "0.3.1",
  "description": "minimum bilibili apk",
  "unpkg": "docs/version.json",
  "files": [
    "apk",
    "docs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "github:lovetingyuan/minibili",
  "homepage": "https://lovetingyuan.github.io/minibili/",
  "scripts": {
    "start": "expo start",
    "android": "adb devices -l && npx expo start --android",
    "tw": "npx react-native-tailwindcss-start",
    "tw:dev": "npx yalc update && npm run tw -- --write=styles.tw.css",
    "ios": "adb devices -l && npx expo start --ios",
    "web": "expo start --web",
    "build:android": "npm run test && zx scripts/build.mjs",
    "build:android:preview": "cross-env APP_VARIANT=preview eas build --platform android --profile preview --non-interactive",
    "update": "zx scripts/hot-update.mjs",
    "doctor": "npx -y expo-doctor@latest",
    "docs": "npx vite docs",
    "format": "npx prettier src scripts --write",
    "test": "npx vitest run",
    "lint": "eslint --ext ts,tsx,js,jsx --fix src scripts",
    "aa": "npx metro get-dependencies index.js --platform android --output tmp/aa.txt",
    "update-expo": "npm i -g eas-cli && npm install expo && npx expo install --fix && npm run doctor",
    "bundle-analyze": "npx react-native-bundle-visualizer --platform android --entry-file index.js --expo true"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "@sentry/react-native": "5.17.0",
    "@shopify/flash-list": "1.6.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "expo": "~50.0.7",
    "expo-application": "~5.8.3",
    "expo-background-fetch": "~11.8.1",
    "expo-clipboard": "~5.0.1",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-image": "~1.10.6",
    "expo-keep-awake": "~12.8.2",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-updates": "~0.24.11",
    "nativewind": "^2.0.11",
    "p-queue": "6.6.2",
    "photoswipe": "^5.4.3",
    "react": "18.2.0",
    "react-atomic-context": "^2.0.0",
    "react-native": "0.73.4",
    "react-native-material-menu": "^2.0.0",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-webview": "13.6.4",
    "spark-md5": "^3.0.2",
    "swr": "^2.2.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/eslint-config": "^0.74.0",
    "@tingyuan/react-native-tailwindcss": "^0.0.15",
    "@types/react": "~18.2.14",
    "@types/spark-md5": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.0.3",
    "download-cli": "^1.1.1",
    "eslint": "^8.37.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "open": "8.4.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "semver": "^7.5.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.3",
    "vitest": "^1.1.0",
    "zx": "^7.2.1"
  },
  "knip": {
    "$schema": "https://unpkg.com/knip@2/schema.json",
    "entry": [
      "*.js"
    ],
    "project": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "scripts/*"
    ]
  },
  "config": {
    "versionCode": 62,
    "changelog": "修复黑暗模式切换"
  },
  "gitHead": "8536929"
}
