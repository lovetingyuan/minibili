{
  "name": "minibili",
  "version": "0.0.37-41",
  "main": "node_modules/expo/AppEntry.js",
  "description": "minimum bilibili apk",
  "unpkg": "docs/version.json",
  "files": [
    "dist",
    "docs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "github:lovetingyuan/minibili",
  "homepage": "https://lovetingyuan.github.io/minibili/",
  "scripts": {
    "start": "expo start",
    "android": "adb devices -l && npx expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "npm run test && zx scripts/build.mjs",
    "build:android:preview": "npm run test && cross-env APP_VARIANT=preview zx scripts/build-preview.mjs",
    "doctor": "expo-cli doctor --fix-dependencies",
    "docs": "npx vite docs",
    "format": "npx prettier src --write",
    "test": "npx vitest run",
    "lint": "eslint --ext ts,tsx,js,jsx --fix src"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "9.3.7",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "@sentry/react-native": "4.13.0",
    "@shopify/flash-list": "1.4.0",
    "expo": "~48.0.10",
    "expo-application": "~5.1.1",
    "expo-background-fetch": "~11.1.1",
    "expo-clipboard": "~4.1.2",
    "expo-constants": "~14.2.1",
    "expo-device": "~5.2.1",
    "expo-keep-awake": "~12.0.1",
    "expo-notifications": "~0.18.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "p-queue": "6.6.2",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-material-menu": "^2.0.0",
    "react-native-pager-view": "6.1.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-webview": "11.26.0",
    "sentry-expo": "~6.1.0",
    "swr": "^2.1.3",
    "throttle-debounce": "^5.0.0",
    "valtio": "^1.10.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.71.4",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-plugin-valtio": "^0.6.2",
    "prettier": "^2.8.7",
    "semver": "^7.5.1",
    "typescript": "^4.9.4",
    "vitest": "^0.30.1",
    "zx": "^7.2.1"
  },
  "knip": {
    "$schema": "https://unpkg.com/knip@2/schema.json",
    "entry": [
      "App.ts",
      "app.config.js"
    ],
    "project": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "scripts/*"
    ]
  }
}
