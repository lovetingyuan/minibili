    "es6-promise-plugin": "^4.2.2",
    "lru-cache": "^6.0.0",
    "normalize.css": "^8.0.1",
    "vue": "3.0.4"
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.4",
    "@types/node": "^14.14.12",
    "@vue/compiler-sfc": "3.0.4",
