{
  "name": "minibili",
  "version": "0.2.3",
  "description": "minimum bilibili apk",
  "unpkg": "docs/version.json",
  "files": [
    "apk",
    "docs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "github:lovetingyuan/minibili",
  "homepage": "https://lovetingyuan.github.io/minibili/",
  "scripts": {
    "start": "expo start",
    "_tailwind": "npx tailwindcss -i ./src/tailwind.css -o ./node_modules/tailwindcss/output.css --watch",
    "android": "adb devices -l && npm run _tailwind && npx expo start --android",
    "ios": "adb devices -l && npx expo start --ios",
    "web": "expo start --web",
    "build:android": "npm run test && zx scripts/build.mjs",
    "build:android:preview": "cross-env APP_VARIANT=preview eas build --platform android --profile preview --non-interactive",
    "update": "zx scripts/hot-update.mjs",
    "doctor": "npx -y expo-doctor@latest",
    "docs": "npx vite docs",
    "format": "npx prettier src scripts --write",
    "test": "npx vitest run",
    "lint": "eslint --ext ts,tsx,js,jsx --fix src scripts"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/netinfo": "9.3.10",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "@sentry/react-native": "5.10.0",
    "@shopify/flash-list": "1.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "expo": "~49.0.21",
    "expo-application": "~5.3.0",
    "expo-background-fetch": "~11.3.0",
    "expo-clipboard": "~4.3.1",
    "expo-constants": "~14.4.2",
    "expo-device": "~5.4.0",
    "expo-image": "~1.3.5",
    "expo-keep-awake": "~12.3.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.19",
    "nativewind": "^2.0.11",
    "p-queue": "6.6.2",
    "photoswipe": "^5.4.3",
    "react": "18.2.0",
    "react-atomic-context": "^2.0.0",
    "react-native": "0.72.6",
    "react-native-material-menu": "^2.0.0",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-webview": "13.2.2",
    "sentry-expo": "~7.1.0",
    "spark-md5": "^3.0.2",
    "swr": "^2.2.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/eslint-config": "^0.74.0",
    "@types/react": "~18.2.14",
    "@types/spark-md5": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "css-to-react-native-transform": "^2.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "semver": "^7.5.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.3",
    "vitest": "^1.1.0",
    "zx": "^7.2.1"
  },
  "knip": {
    "$schema": "https://unpkg.com/knip@2/schema.json",
    "entry": [
      "*.js"
    ],
    "project": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "scripts/*"
    ]
  },
  "config": {
    "versionCode": 59,
    "changelog": "修复动态列表问题"
  },
  "gitHead": "07bf34e"
}
